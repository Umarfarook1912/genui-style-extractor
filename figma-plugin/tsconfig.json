{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "./",
    "rootDir": "./",
    "types": ["@figma/plugin-typings"]
  },
  "include": ["code.ts"],
  "exclude": ["node_modules"]
}

